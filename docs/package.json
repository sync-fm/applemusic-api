{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "dev": "TYPEDOC_WATCH=true docusaurus start",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "rm -rf docs/api && docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "build": "bun run clear && bun ./scripts/update-status.ts && docusaurus build"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.1",
    "@docusaurus/plugin-sitemap": "^3.9.1",
    "@docusaurus/preset-classic": "3.9.1",
    "@getcanary/docusaurus-theme-search-pagefind": "^1.0.2",
    "@getcanary/web": "^1.0.12",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "docusaurus-plugin-typedoc": "^1.4.2",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "smooth-shadow": "^1.0.0",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.9.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.1",
    "@docusaurus/tsconfig": "3.9.1",
    "@docusaurus/types": "3.9.1",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
